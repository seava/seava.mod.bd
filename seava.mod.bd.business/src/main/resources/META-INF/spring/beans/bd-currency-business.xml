<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd">
	
		<bean id="Currency" scope="singleton" lazy-init="true"
			class="seava.bd.business.impl.currency.Currency_Service"/>				
	
		<bean id="CurrencyXRateProvider" scope="singleton" lazy-init="true"
			class="seava.bd.business.impl.currency.CurrencyXRateProvider_Service"/>				
	
		<bean id="CurrencyXRate" scope="singleton" lazy-init="true"
			class="seava.bd.business.impl.currency.CurrencyXRate_Service"/>				
	
		<bean id="CurrencyXRateAverage" scope="singleton" lazy-init="true"
			class="seava.bd.business.impl.currency.CurrencyXRateAverage_Service"/>				
</beans>
