<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd">
	
		<bean id="Country" scope="singleton" lazy-init="true"
			class="seava.bd.business.impl.geo.Country_Service"/>				
	
		<bean id="Region" scope="singleton" lazy-init="true"
			class="seava.bd.business.impl.geo.Region_Service"/>				
	
		<bean id="City" scope="singleton" lazy-init="true"
			class="seava.bd.business.impl.geo.City_Service"/>				
	
		<bean id="Location" scope="singleton" lazy-init="true"
			class="seava.bd.business.impl.geo.Location_Service"/>				
</beans>
