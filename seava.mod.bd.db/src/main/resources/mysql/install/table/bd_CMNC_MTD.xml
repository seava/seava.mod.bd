<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

	<changeSet author="nan21" id="TBL-bd_CMNC_MTD">
		<createTable tableName="bd_CMNC_MTD">
			<column name="ACTIVE" type="int(1)">
				<constraints nullable="false"/>
			</column>
			<column name="CLIENTID" type="varchar(64)">
				<constraints nullable="false"/>
			</column>
			<column name="CREATEDAT" type="datetime">
				<constraints nullable="false"/>
			</column>
			<column name="CREATEDBY" type="varchar(32)">
				<constraints nullable="false"/>
			</column>
			<column name="ID" type="varchar(64)">
				<constraints nullable="false"/>
			</column>
			<column name="MODIFIEDAT" type="datetime">
				<constraints nullable="false"/>
			</column>
			<column name="MODIFIEDBY" type="varchar(32)">
				<constraints nullable="false"/>
			</column>
			<column name="NOTES" type="text"/>
			<column name="REFID" type="varchar(64)">
				<constraints nullable="false"/>
			</column>
			<column name="TARGETALIAS" type="varchar(64)">
				<constraints nullable="false"/>
			</column>
			<column name="TARGETREFID" type="varchar(64)">
				<constraints nullable="false"/>
			</column>
			<column name="TARGETTYPE" type="varchar(255)"/>
			<column name="VALIDFROM" type="date"/>
			<column name="VALIDTO" type="date"/>
			<column name="VALUE" type="varchar(255)">
				<constraints nullable="false"/>
			</column>
			<column name="VERSION" type="bigint(10)"/>
			<column name="TYPE_ID" type="varchar(64)">
				<constraints nullable="false"/>
			</column>
		</createTable>
	</changeSet>

	<changeSet author="nan21" id="PK-bd_CMNC_MTD">
		<addPrimaryKey 
			constraintName="PK_bd_CMNC_MTD"
			tableName="bd_CMNC_MTD"
			columnNames="ID"/>
	</changeSet>

</databaseChangeLog>
