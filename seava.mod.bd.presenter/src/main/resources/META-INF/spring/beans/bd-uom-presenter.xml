		<property name="entityClass" value="seava.bd.domain.impl.uom.Uom"/>
		<property name="readOnly" value="true" />
	</bean>
	
	<!-- ==================== UomConversion ==================== -->
		
	<bean id="UomConversion_Ds" scope="singleton" lazy-init="true"
		class="seava.j4e.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="seava.bd.presenter.impl.uom.model.UomConversion_Ds"/>
		<property name="entityClass" value="seava.bd.domain.impl.uom.UomConversion"/>
