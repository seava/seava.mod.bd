		<property name="modelClass" value="seava.bd.presenter.impl.uom.model.UomTypeLov_Ds"/>
		<property name="entityClass" value="seava.bd.domain.impl.uom.UomType"/>
		<property name="readOnly" value="true" />
	</bean>
	
	<!-- ==================== Uom ==================== -->
		
	<bean id="Uom_Ds" scope="singleton" lazy-init="true"
		class="seava.j4e.presenter.service.ds.DefaultEntityDsService">
		<property name="modelClass" value="seava.bd.presenter.impl.uom.model.Uom_Ds"/>
		<property name="entityClass" value="seava.bd.domain.impl.uom.Uom"/>
	</bean>
	
